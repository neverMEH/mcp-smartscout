{
  "name": "@smartscout/mcp-domo",
  "version": "1.0.0",
  "description": "MCP server for querying SmartScout Domo tables",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:v2": "tsc && echo 'Built v2 server - use dist/server-v2.js'",
    "build:http": "tsc",
    "start": "node dist/server.js",
    "start:v2": "node dist/server-v2.js",
    "start:http": "node dist/http-server.js",
    "dev": "tsx src/server.ts",
    "dev:v2": "tsx src/server-v2.ts",
    "dev:http": "tsx src/http-server.ts",
    "watch": "tsc -w",
    "bridge": "node dist/http-bridge.js"
  },
  "keywords": [
    "mcp",
    "smartscout",
    "domo",
    "amazon",
    "marketplace"
  ],
  "author": "SmartScout",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
